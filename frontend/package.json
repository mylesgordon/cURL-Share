		"svelte-htm": "^1.2.0",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"unplugin-icons": "^0.15.2",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
